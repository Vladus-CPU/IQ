   "source": [
    "import torch\n",
    "from diffusers import StableDiffusionXLPipeline, EulerDiscreteScheduler, DPMSolverMultistepScheduler\n",
    "from diffusers.schedulers import (\n",
    "    DDIMScheduler,\n",
    "    DPMSolverMultistepScheduler,\n",
    "    EulerAncestralDiscreteScheduler,\n",
    "    EulerDiscreteScheduler,\n",
    "    HeunDiscreteScheduler,\n",
    "    KDPM2DiscreteScheduler,\n",
    "    KDPM2AncestralDiscreteScheduler,\n",
    ")\n",
    "import numpy as np\n",
    "from PIL import Image\n",
    "import cv2\n",
    "import os"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "source": [
    "# Dictionary of available samplers\n",
    "SCHEDULERS = {\n",
    "    \"DPM++ 2M\": DPMSolverMultistepScheduler,\n",
    "    \"Euler\": EulerDiscreteScheduler,\n",
    "    \"Euler A\": EulerAncestralDiscreteScheduler,\n",
    "    \"Heun\": HeunDiscreteScheduler,\n",
    "    \"DDIM\": DDIMScheduler,\n",
    "    \"KDPM2\": KDPM2DiscreteScheduler,\n",
    "    \"KDPM2 A\": KDPM2AncestralDiscreteScheduler,\n",
    "}\n",
    "\n",
    "# Load the model\n",
    "def load_model(sampler_name=\"Euler\"):\n",
    "    scheduler = SCHEDULERS[sampler_name].from_pretrained(\n",
    "        \"Laxhar/noobai-XL-1.1\",\n",
    "        subfolder=\"scheduler\",\n",
    "        torch_dtype=torch.float16\n",
    "    )\n",
    "    \n",
    "    pipe = StableDiffusionXLPipeline.from_pretrained(\n",
    "        \"Laxhar/noobai-XL-1.1\",\n",
    "        scheduler=scheduler,\n",
    "        torch_dtype=torch.float16,\n",
    "        variant=\"fp16\",\n",
    "        use_safetensors=True\n",
    "    )\n",
    "    \n",
    "    # Remove token limit\n",
    "    pipe.tokenizer.model_max_length = 1000000\n",
    "    pipe.tokenizer_2.model_max_length = 1000000\n",
    "    \n",
    "    return pipe.to(\"cuda\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "source": [
    "# HiRes upscaler implementation\n",
    "def hires_fix_upscale(image, scale=2, creativity=1.0, steps=20):\n",
    "    # Convert PIL to CV2\n",
    "    img = np.array(image)\n",
    "    img = cv2.cvtColor(img, cv2.COLOR_RGB2BGR)\n",
    "    \n",
    "    # Calculate new dimensions\n",
    "    h, w = img.shape[:2]\n",
    "    new_h, new_w = int(h * scale), int(w * scale)\n",
    "    \n",
    "    # Apply 8x-NKMD-Superscale\n",
    "    for _ in range(steps):\n",
    "        img = cv2.resize(img, (new_w, new_h), interpolation=cv2.INTER_LANCZOS4)\n",
    "        # Apply creative enhancement\n",
    "        img = cv2.detailEnhance(img, sigma_s=creativity, sigma_r=0.15)\n",
    "    \n",
    "    # Convert back to PIL\n",
    "    img = cv2.cvtColor(img, cv2.COLOR_BGR2RGB)\n",
    "    return Image.fromarray(img)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "source": [
    "def generate_image(\n",
    "    prompt,\n",
    "    negative_prompt=\"\",\n",
    "    width=1024,\n",
    "    height=1024,\n",
    "    num_steps=30,\n",
    "    guidance_scale=7.5,\n",
    "    seed=None,\n",
    "    sampler_name=\"Euler\",\n",
    "    use_hires=False,\n",
    "    hires_scale=2,\n",
    "    hires_creativity=1.0,\n",
    "    hires_steps=20\n",
    "):\n",
    "    # Load or get existing model\n",
    "    pipe = load_model(sampler_name)\n",
    "    \n",
    "    # Set random seed if not provided\n",
    "    if seed is None:\n",
    "        seed = torch.randint(0, 2**32 - 1, (1,)).item()\n",
    "    \n",
    "    # Set the generator\n",
    "    generator = torch.Generator(\"cuda\").manual_seed(seed)\n",
    "    \n",
    "    # Generate the image\n",
    "    image = pipe(\n",
    "        prompt=prompt,\n",
    "        negative_prompt=negative_prompt,\n",
    "        width=width,\n",
    "        height=height,\n",
    "        num_inference_steps=num_steps,\n",
    "        guidance_scale=guidance_scale,\n",
    "        generator=generator\n",
    "    ).images[0]\n",
    "    \n",
    "    # Apply HiRes fix if requested\n",
    "    if use_hires:\n",
    "        image = hires_fix_upscale(\n",
    "            image,\n",
    "            scale=hires_scale,\n",
    "            creativity=hires_creativity,\n",
    "            steps=hires_steps\n",
    "        )\n",
    "    \n",
    "    return image, seed"
   ]
  },
